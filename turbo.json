{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["//#tsc"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "//#tsc": {
      "outputs": ["dist-types/**"]
    }
  },
  "globalDependencies": ["tsconfig.json"]
}
