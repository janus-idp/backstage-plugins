{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "lint": {}
  },
  "globalDependencies": ["tsconfig.json"]
}
