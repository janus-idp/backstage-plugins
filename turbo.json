{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".prettierrc.js", ".eslintrc.js", "tsconfig.json", "yarn.lock"],
  "tasks": {
    "start": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["tsc", "^build", "export-dynamic"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "export-dynamic": {
      "cache": false,
      "dependsOn": ["tsc"]
    },
    "lint:check": {},
    "lint:fix": {
      "cache": false
    },
    "prettier:check": {},
    "prettier:fix": {
      "cache": false
    },
    "tsc": {
      "outputs": ["../../dist-types/**"],
      "dependsOn": ["^tsc"]
    }
  }
}
