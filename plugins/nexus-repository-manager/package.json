    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "18.18.5",
    "cross-fetch": "4.0.0",
    "msw": "1.3.2",
    "openapi-typescript-codegen": "0.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "files": [
    "dist",
    "config.d.ts"
