    "openapi-backend": "^5.10.5",
    "openapi-types": "^12.1.3",
    "winston": "^3.11.0",
    "yn": "^5.0.0",
    "moment": "^2.29.4",
    "ajv-formats": "^2.1.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {},
  "bundleDependencies": true,
  "peerDependencies": {
    "@backstage/backend-app-api": "^0.6.1",
